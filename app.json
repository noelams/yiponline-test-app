{
  "expo": {
    "name": "yiponline-test-app",
    "slug": "yiponline-test-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/yiponline-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/yiponline-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/yiponline-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.noelams.yiponlinetestapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e0a95f2b-1472-4250-8c40-515e099cd1f2"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "useLegacyPackaging": true,
            "enableProguardInReleaseBuilds": true,
            "proguardRules": "proguard-rules.pro"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ]
  }
}
